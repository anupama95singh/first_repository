class Driver{
	String name;
	int learningLicenseNo;
	String address;
	
    void driverRecord(String n, int ll, String a) {
    	name = n;
    	learningLicenseNo = ll;
    	address = a;
    }
    void display() {
    	System.out.println(name +" "+learningLicenseNo+" "+address);
    }
}


public class TestDriver {
	public static void main(String args[]) {
		System.out.println("Hello Java");
		Driver d1 = new Driver();
		Driver d2 = new Driver();
		d1.driverRecord("anu",100,"banglore");
		d2.driverRecord("mihir",150,"banglore");
		d1.display();
		d2.display();
		
	}
}

